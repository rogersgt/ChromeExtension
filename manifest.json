{
  "manifest_version": 2,

  "name": "New York Times Chrome Extension",
  "description": "This extension shows the most popular Times articles",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://nytimes.com/"
  ],
  "browser_action": {
  "default_icon": "icon.png",
  "default_popup": "index.html",
  "default_title": "NYT Most Popular Articles"
}
}
